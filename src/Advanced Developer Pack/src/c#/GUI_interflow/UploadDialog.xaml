<Window x:Name="UploadDialog" x:Class="GUI_interflow.UploadWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GUI_interflow"
        mc:Ignorable="d"
        Title="Upload File" Height="307.66" Width="300" ResizeMode="NoResize">
    <Grid>
        <TextBox x:Name="OneIndicatorContent" Margin="10,10,10,34" TextWrapping="Wrap" AcceptsReturn="True" VerticalScrollBarVisibility="Auto"/>
        <Button x:Name="Button_Import_file" Content="Import File" Margin="10,0,156,9" Height="20" VerticalAlignment="Bottom" Click="Button_Import_file_Click"/>
        <Button x:Name="Button_Upload_Submit" Content="Submit" Margin="156,0,10,9" Height="20" VerticalAlignment="Bottom" Click="Button_Upload_Submit_Click"/>

    </Grid>
</Window>
